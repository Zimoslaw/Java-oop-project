<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.collections.*?>
<?import kingdoms.kingdoms.BaseObject?>

<?import javafx.scene.control.cell.CheckBoxListCell?>
<GridPane alignment="CENTER" xmlns:fx="http://javafx.com/fxml" hgap="10" vgap="10" prefHeight="500.0" prefWidth="300.0"
          fx:controller="kingdoms.kingdoms.AddDuchyViewController">

    <Label text="Dodaj nowe księstwo" alignment="CENTER" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="2"><font><Font name="Arial" size="18" /></font></Label>
    <Label text="Nazwa" alignment="CENTER_RIGHT" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <TextField fx:id="name" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <Label text="Centralizacja" alignment="CENTER_RIGHT" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <ComboBox fx:id="centralization" GridPane.columnIndex="1" GridPane.rowIndex="2">

    </ComboBox>
    <Label text="Władca" alignment="CENTER_RIGHT" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
    <ComboBox fx:id="ruler" GridPane.columnIndex="1" GridPane.rowIndex="3">

    </ComboBox>
    <Label text="Prowincje" alignment="CENTER_RIGHT" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
    <ListView fx:id="provinces" GridPane.columnIndex="1" GridPane.rowIndex="4" prefHeight="100">

    </ListView>
    <Button text="Dodaj" onAction="#Submit" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
    <TextArea fx:id="infos" GridPane.columnIndex="0" GridPane.rowIndex="6" GridPane.columnSpan="2" editable="false"/>
</GridPane>